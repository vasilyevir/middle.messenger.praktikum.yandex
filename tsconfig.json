{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "typeRoots": [
      "node_modules/@types",
      "app.d.ts"
    ],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "include": []
}

